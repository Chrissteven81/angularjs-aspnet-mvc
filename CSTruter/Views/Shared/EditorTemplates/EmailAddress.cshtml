@model object

<div class="form-group">
    @Html.LabelFor(m => m, new { @class = "control-label col-sm-2" })
    <div class="col-sm-6">
        @Html.TextBoxFor(m => m, new
        {
            type = "email",
            @class = "form-control",
            ng_model = ViewData.TemplateInfo.HtmlFieldPrefix
        })
        @Html.ValidationAngularMessageFor(m => m)
    </div>
</div>